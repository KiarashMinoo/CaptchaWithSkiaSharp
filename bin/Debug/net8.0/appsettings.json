{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CaptchaOptions": {
    "IsEnabled": true,
    "LifespanInMinutes": 2,
    "Width": 180,
    "Height": 50,
    "ImageQuality": 80,
    "EncoderType": "Png",
    "FontFamilies": [],
    "FontSize": 29,
    "FontStyle": "Italic",
    "BackgroundColor": "#FFFFFF",
    "TextColor": [
      "#0000FF",
      "#000000",
      "#A52A2A",
      "#808080",
      "#008000"
    ],
    "DrawLines": 5,
    "DrawLinesColor": [
      "#0000FF",
      "#000000",
      "#A52A2A",
      "#808080",
      "#008000"
    ],
    "MinLineThickness": 0.7,
    "MaxLineThickness": 2.0,
    "NoiseRate": 800,
    "NoiseRateColor": [
      "#808080"
    ]
  }
}
